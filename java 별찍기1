package ch02;

public class ch01_01 {

	public static void main(String[] args) {
		//별찍기
		for(int i = 1; i <= 10; i++) {
			if(i<=5) {
			for(int j = 1; j <= i; j++) {
				System.out.printf("*");
			}
			System.out.println();
			}else if(i>6) {
				for(int j = 10; i <= j; j--) {
					System.out.printf("*");
				}
				System.out.println();
			}
			
		}
	}

}
