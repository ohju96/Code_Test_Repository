public class ch02_03 {

	public static void main(String[] args) {

		int i = 3;
		while(i<=8) {
			System.out.println();
			if( i == 3 || i == 8) {
				System.out.println("[" + i + "ë‹¨]");
			}
			int j = 1;
			while(j<=9) {
				System.out.println(i + "x" + j + "=" + i*j);
				j++;
			}
			i++;
		}
	}

}
