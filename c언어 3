#include <stdio.h>
int main()
{
	int a, b, c, d;
	int result;

	printf("첫 번째 게산할 값을 입력하세요 ==>");
	scanf("%d", &a);

	printf("두 번째 게산할 값을 입력하세요 ==>");
	scanf("%d", &b);

	printf("세 번째 게산할 값을 입력하세요 ==>");
	scanf("%d", &c);

	printf("네 번째 게산할 값을 입력하세요 ==>");
	scanf("%d", &d);
	
	result = a + b + c + d;

		printf("합계는 %d 입니다.\n", result);
}
