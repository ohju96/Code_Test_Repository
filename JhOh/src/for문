
public class Ex12 {

	public static void main(String[] args) {

		int i;

		for (i = 0; i < 3; i++) {
			System.out.println("반복을 위한 i 값 : " + i);
			System.out.println("또 안녕하세요 \n");
		}

		System.out.println("\n\n");

		for (i = 1; i <= 3; i++) {
			// <= 연산을 두 번 하기 때문에 위에서 < 한 번 쓰는 게 더 좋다.
			System.out.println("반복 횟수 : " + i);
		}
	}
}
