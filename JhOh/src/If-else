public class EXam09 {

	public static void main(String[] args) {
		
		int a = 100;
		
		if (a<100) {
			System.out.println("100보다 작군요");
			System.out.println("참이면 보이죠?");
		} else {
			System.out.println("100보다 크군요");
			System.out.println("거짓이면 보이죠?");
		}
	System.out.println("난 조건절에 상관업이 무조건 보인다.");
	}

}
